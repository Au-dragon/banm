        <activity android:name=".activity.CardActivity" />
        <activity android:name=".activity.TourismActivity" />
        <activity android:name=".activity.CollectActivity" />
        <activity android:name=".activity.AttentionActivity" />
        <activity android:name=".activity.CallActivity" />
        <activity android:name=".activity.TalkActivity" />
